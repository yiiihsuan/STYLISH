    "basePath": "/api/1.0", 
    "schemes": ["http"], 
    "paths": {
      "/createProduct": {
        "post": {
          "summary": "Create a new product",
          "description": "Create a new product with main_image and images.",
          "consumes": ["multipart/form-data"],
          "produces": ["application/json"],
          "parameters": [
            {
              "name": "main_image",
              "in": "formData",
              "type": "file",
              "description": "Main product image",
              "required": true
        },
        {
            "name": "image",
            "in": "formData",
            "type": "array",
            "items": {
                "type": "file"
            },
            "description": "Additional product images",
            "required": false
            },
            {
                "name": "data",
                "in": "formData",
                "type": "string",
                "description": "Product",
                "required": false,
                "default": "{\"id\": 1234, \"category\": \"men\", \"title\": \"厚實毛呢格子外套\", \"description\": \"高抗寒素材選用，保暖也時尚有型\", \"price\": 2200, \"texture\": \"棉、聚脂纖維\", \"wash\": \"手洗（水溫40度\", \"place\": \"韓國\", \"note\": \"實品顏色以單品照為主\", \"story\": \"你絕對不能錯過的超值商品\", \"colors\": [{ \"code\":\"334455\", \"name\":\"深藍\" }], \"sizes\": [\"S\", \"M\", \"L\"], \"variants\": [{ \"color_code\":\"334455\", \"size\":\"S\", \"stock\":5 }, { \"color_code\":\"334455\", \"size\":\"M\", \"stock\":10 }, { \"color_code\":\"334455\", \"size\":\"L\", \"stock\":0 } ]}"
              }
          ],
          "responses": {
            "200": {
              "description": "Product created successfully",
              "schema": {
                "$ref": "#/definitions/Product"
              }
            },
            "400": {
              "description": "Bad request"
            },
            "500": {
              "description": "Internal Server error"
            }
          }
        }
      },
      "/products/details": {
        "get": {
          "summary": "Product Details API",
          "description": "Get details of a specific product.",
          "produces": ["application/json"],
          "parameters": [
            {
              "name": "id",
              "in": "query",
              "description": "Product Id",
              "required": true,
              "type": "integer",
              "default":20
            }
          ],
          "responses": {
            "200": {
              "description": "Success",
              "schema": {
                "$ref": "#/definitions/ProductDetailsResponse"
              }
            },
            "400": {
              "description": "Bad request",
              "schema": {
                "$ref": "#/definitions/ClientErrorResponse"
              }
            },
            "500": {
              "description": "Internal Server error",
              "schema": {
                "$ref": "#/definitions/ServerErrorResponse"
              }
            }
          }
        }
      },
      "/products/search": {
        "get": {
          "summary": "Product Search API",
          "description": "Search for products based on certain criteria.",
          "produces": ["application/json"],
          "parameters": [
            {
              "name": "keyword",
              "in": "query",
              "description": "The search keyword",
              "required": true,
              "type": "string",
              "default":"洋裝"
            },
            {
              "name": "paging",
              "in": "query",
              "description": "Page number for pagination (optional)",
              "required": false,
              "type": "integer"
            }
          ],
          "responses": {
            "200": {
              "description": "Product search",
              "schema": {
                "$ref": "#/definitions/ProductSearchResponse"
              }
            },
            "400": {
              "description": "Bad request",
              "schema": {
                "$ref": "#/definitions/ClientErrorResponse"
              }
            },
            "500": {
              "description": "Internal Server error",
              "schema": {
                "$ref": "#/definitions/ServerErrorResponse"
              }
            }
          }
        }
      },
      "/products/{category}": {
        "get": {
          "summary": "Get product list by category",
          "description": "Get a list of products by category.",
          "produces": ["application/json"],
          "parameters": [
            {
              "name": "category",
              "in": "path",
              "description": "Product category",
              "required": true,
              "type": "string",
              "enum": ["all", "men", "women", "accessories"],
              "default": "all"
            },
            {
              "name": "paging",
              "in": "query",
              "description": "Page number for pagination (optional)",
              "required": false,
              "type": "integer"
            }
          ],
          "responses": {
            "200": {
              "description": "Success",
              "schema": {
                "$ref": "#/definitions/Productget"
              }
            },
            "400": {
              "description": "Bad request"
            },
            "500": {
              "description": "Internal Server error"
            }
          }
        }
      },
      "/user/signup": {
        "post": {
          "summary": "User Sign Up",
          "description": "Create a new user account",
          "consumes": ["application/json"],
          "produces": ["application/json"],
          "parameters": [
            {
              "in": "body",
              "name": "body",
              "required": true,
              "schema": {
                "type": "object",
                "properties": {
                  "name": { "type": "string" },
                  "email": { "type": "string" },
                  "password": { "type": "string" }
                },
                "required": ["name", "email", "password"]
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Success",
              "schema": {
                "$ref": "#/definitions/SignUpResponse"
              }
            },
            "400": { "description": "Client Error" },
            "409": { "description": "Email Already Exists" },
            "500": { "description": "Server Error" }
          }
        }
      },
      "/user/signin": {
        "post": {
          "summary": "User Sign In",
          "description": "Sign in a user",
          "consumes": ["application/json"],
          "produces": ["application/json"],
          "parameters": [
            {
              "in": "body",
              "name": "body",
              "required": true,
              "schema": {
                "$ref": "#/definitions/SignInBody"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Success",
              "schema": {
                "$ref": "#/definitions/SuccessResponse"
              }
            },
            "400": { "description": "Client Error" },
            "403": { "description": "Sign In Failed" },
            "500": { "description": "Server Error" }
          }
        }
      },
      "/user/profile": {
        "get": {
          "summary": "Get User Profile",
          "description": "Retrieve the user’s profile information",
          "produces": ["application/json"],
          "parameters": [
            {
              "in": "header",
              "name": "Authorization",
              "type": "string",
              "required": true
            }
          ],
          "responses": {
            "200": {
              "description": "Success",
              "schema": {
                "$ref": "#/definitions/UserProfileResponse"
              }
            },
            "401": { "description": "Client Error (No token)" },
            "403": { "description": "Client Error (Wrong token)" },
            "500": { "description": "Server Error" }
          }
        }
      }
    },
